@model List<collegeEventTracker2.Models.Event>
@{
    ViewData["Title"] = "Events";
}

<h1>Events</h1>
@foreach (var item in Model){
<div class="card">
    <h2>@item.Title</h2>
    <p>@item.Description</p>
    <p> Venue: @item.Venue</p>
    <p> Date: @item.DateTime</p>
    <p> Organizer: @item.OrganizerId</p>
    <p> Max Participants: @item.MaxParticipants</p>
    <p> Is Team Event: @item.IsTeamEvent</p>
</div>
}
